<?xml version="1.0" encoding="ISO-8859-1"?>
<project
    name="${project.name}"
    xmlns="http://maven.apache.org/DECORATION/1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/DECORATION/1.0.0 http://maven.apache.org/xsd/decoration-1.0.0.xsd"
>

    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>${fluido-skin.version}</version>
    </skin>

    <custom>
        <fluidoSkin>
            <sideBarEnabled>true</sideBarEnabled>
            <googlePlusOne />
            <gitHub>
                <projectId>hakan42/android-commons</projectId>
                <ribbonOrientation>right</ribbonOrientation>
                <ribbonColor>gray</ribbonColor>
            </gitHub>
            <twitter>
                <user>hakan42</user>
            </twitter>
            <facebookLike />
        </fluidoSkin>
    </custom>

    <body>
        <menu name="Manual Content">
            <item name="Attribution" href="attribution.html"/>
        <!--
            <item name="APT Format" href="format.html"/>
            <item name="Xdoc Example" href="xdoc.html"/>
            <item name="FAQ" href="faq.html"/>
        -->
        </menu>

        <!--
        <links>
            <item name="akquinet" href="http://www.akquinet.de/en"/>
        </links>
        -->

        <menu ref="reports" />
        <menu ref="modules" />
    </body>
</project>
